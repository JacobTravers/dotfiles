#!/bin/sh
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/c++
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/c89-gcc
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/c99-gcc
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/cc
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/g++
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/g++-10
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/gcc
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/gcc-10
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/x86_64-linux-gnu-g++
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/x86_64-linux-gnu-g++-10
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/x86_64-linux-gnu-gcc
doas ln "$HOME"/.local/share/cargo/bin/sccache "$HOME"/.local/bin/x86_64-linux-gnu-gcc-10
